{
  "version": 3,
  "builds": [
    {
      "src": "api/sendEmail.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/sendEmail",
      "destination": "/api/sendEmail.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
